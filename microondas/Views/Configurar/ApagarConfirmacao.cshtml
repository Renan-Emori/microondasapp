@model PreDefinidoModel


<div class="text-center">
    <h3>Realmente deseja apagar o contato (@Model.Nome)?</h3>
</div>

<a class="btn btn-danger btn-block" asp-action="Apagar" asp-controller="Configurar" asp-route-id="@Model.Id" role="button">Confirmar exclusão</a>
<a class="btn btn-primary btn-block" asp-action="Configurar" asp-controller="Configurar" role="button">Cancelar exclusão</a>